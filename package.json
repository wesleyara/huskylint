{
  "name": "huskylint",
  "version": "1.0.6",
  "description": "Executable to configure husky, lint staged and commitlint.",
  "bin": "lib/script.js",
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "dev": "ts-node src/index.ts",
    "build": "rimraf lib && tsc --declaration",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepare": "husky",
    "postversion": "git push && git push --tags && rm -rf build/temp"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint . --fix"
    ]
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesleyara/huskylint.git"
  },
  "keywords": [
    "typescript",
    "javascript"
  ],
  "author": "Wesley Ara√∫jo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wesleyara/huskylint/issues"
  },
  "homepage": "https://github.com/wesleyara/huskylint#readme",
  "dependencies": {
    "axios": "^1.2.0",
    "chalk": "3.0.0",
    "inquirer": "8.0.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "utils-react": "^1.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.1.0",
    "@eslint/js": "^9.22.0",
    "@types/inquirer": "8.1.0",
    "@types/node": "^18.11.9",
    "@types/shelljs": "^0.8.11",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-perfectionist": "^4.9.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^13.0.4",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "typescript-eslint": "^8.26.0",
    "vitest": "^3.0.8"
  }
}
